<div class="container">
    <hr>
    <h2>Us√∫arios</h2>
    <a class="btn btn-success" href="/admin/users/create">Criar novo usuario</a>
    <hr>
    <table class="table table-bordered">
        <thead>
            <tr> <!--// linha da tabela-->
                <th>ID</th> <!--// coluna da tabela-->
                <th>Email</th>               
            </tr>
        </thead>
        <tbody>
            <% users.forEach(user =>{ %> 
            <tr>
               <td><%= user.id %> </td>
               <td><%= user.email%> </td>                                               
            </tr>          
             <% }) %> 
        </tbody>
    </table>
</div>


